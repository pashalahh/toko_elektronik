              <AuxValues>
                <AuxValue name="JavaCodeGenerator_CreateCodeCustom" type="java.lang.String" value="new javax.swing.JPanel() {&#xa;    @Override&#xa;    protected void paintComponent(Graphics g) {&#xa;        super.paintComponent(g);&#xa;&#xa;        if (listTotal.size() &lt; 2) {&#xa;            g.drawString(&quot;Data tidak cukup untuk grafik (minimal 2 hari)&quot;, 20, 20);&#xa;            return;&#xa;        }&#xa;&#xa;        Graphics2D g2 = (Graphics2D) g;&#xa;        g2.setRenderingHint(&#xa;            RenderingHints.KEY_ANTIALIASING,&#xa;            RenderingHints.VALUE_ANTIALIAS_ON&#xa;        );&#xa;&#xa;        int pad = 40;&#xa;        int w = getWidth() - 2 * pad;&#xa;        int h = getHeight() - 2 * pad;&#xa;&#xa;        // Cari nilai maksimum&#xa;        double max = Collections.max(listTotal);&#xa;&#xa;        double xDiv = (double) w / (listTotal.size() - 1);&#xa;        double yDiv = (double) h / max;&#xa;&#xa;        // Garis grafik&#xa;        g2.setColor(new Color(0, 102, 204));&#xa;        g2.setStroke(new BasicStroke(3f));&#xa;&#xa;        for (int i = 0; i &lt; listTotal.size() - 1; i++) {&#xa;            int x1 = pad + (int) (i * xDiv);&#xa;            int y1 = (getHeight() - pad) - (int) (listTotal.get(i) * yDiv);&#xa;&#xa;            int x2 = pad + (int) ((i + 1) * xDiv);&#xa;            int y2 = (getHeight() - pad) - (int) (listTotal.get(i + 1) * yDiv);&#xa;&#xa;            g2.drawLine(x1, y1, x2, y2);&#xa;            g2.fillOval(x1 - 4, y1 - 4, 8, 8);&#xa;        }&#xa;&#xa;        // Titik terakhir&#xa;        int lastX = pad + (int) ((listTotal.size() - 1) * xDiv);&#xa;        int lastY = (getHeight() - pad)&#xa;        - (int) (listTotal.get(listTotal.size() - 1) * yDiv);&#xa;        g2.fillOval(lastX - 4, lastY - 4, 8, 8);&#xa;&#xa;        // Axis&#xa;        g2.setColor(Color.BLACK);&#xa;        g2.setStroke(new BasicStroke(1f));&#xa;        g2.drawLine(pad, getHeight() - pad, getWidth() - pad, getHeight() - pad); // X&#xa;        g2.drawLine(pad, pad, pad, getHeight() - pad); // Y&#xa;    }&#xa;    }"/>
